<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:icon="@null"
    android:key="topPrefScreen"
    android:title="@string/navigation_settings">

    <app.michaelwuensch.bitbanana.settings.FeePresetPref
        android:defaultValue="1"
        android:key="feePresetFast"
        android:title="@string/settings_fastFee"
        app:iconSpaceReserved="false" />

    <app.michaelwuensch.bitbanana.settings.FeePresetPref
        android:defaultValue="18"
        android:key="feePresetMedium"
        android:title="@string/settings_mediumFee"
        app:iconSpaceReserved="false" />

    <app.michaelwuensch.bitbanana.settings.FeePresetPref
        android:defaultValue="144"
        android:key="feePresetSlow"
        android:title="@string/settings_slowFee"
        app:iconSpaceReserved="false" />
</PreferenceScreen>